page{
    background-color: #ffffff;
    font-family: "Helvetica Neue","Hiragino Sans GB","Microsoft YaHei","\9ED1\4F53",Arial,sans-serif;
    font-size: 32rpx;
}
.navigator-hover{
    background: none;
}

.cart-box{
    padding-bottom: 100rpx;
}
.cart-list{
  width: 400rpx;
    position: relative;
    padding: 20rpx 60rpx 20rpx 285rpx;
    height: 185rpx;
    border-bottom: 1rpx solid #e9e9e9;
    margin: 0 auto;
}
.cart-list .cart-pro-select{
    position: absolute;
    left: 20rpx;
    top: 90rpx;
    width: 45rpx;
    height: 45rpx;
}

.cart-list .cart-thumb{
    position: absolute;
    top: 10rpx;
    left: 85rpx;
    width: 150rpx;
    height: 150rpx;
}

.cart-list {
    display: inline-block;
    width:400rpx;
    height: 105rpx;
    line-height: 50rpx;
    overflow: hidden;
    margin: 0 auto;
}
 .cart-pro-names,
 .cart-pro-name{
    display: inline-block;
    width: 200rpx;
    height:50rpx;
    font-size: 25rpx;
    overflow: hidden;
}
.cart-list .cart-pro-price{
    display: inline-block;
    float: right;
    height: 50rpx;

}
.cart-list .cart-count-box{
    position: absolute;
    left: 285;
    bottom: 20rpx;
    width: 250rpx;
    height: 80rpx;
}
.cart-list .cart-count-box text{
    display: inline-block;
    line-height: 80rpx;
    text-align: center;
}
.cart-count-down{
    font-size:25rpx;
    font-weight: 900;
    width: 30rpx;
    height: 100%;
    line-height: 100%;
    overflow: hidden;
    float: left;
}
.cart-count-add{
    font-size: 50rpx;
    width: 50rpx;
    height: 100%;
    display: inline-block;
    float: left;
}
.cart-count-num{
    width: 150rpx;
    display: inline-block;
    float: left;
}
.cart-del{
    position: absolute;
    right: 20rpx;
    bottom: 0;
    width: 80rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    font-size: 44rpx;
}
.cart-footer{
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
    padding:0 100rpx 0 80rpx;
    box-sizing: border-box;
    background: #AB956D;
    color: #fff;
}
.total-select{
    position: absolute;
    left: 20rpx;
    top: 25rpx;
    width: 45rpx;
    height: 45rpx;
}
.order-icon{
    position: absolute;
    right: 40rpx;
    top: 25rpx;
    width: 45rpx;
    height: 45rpx;
}
.order-icon image,.order-icon navigator{
  display: block;
  width: 45rpx;
  height: 45rpx;
}
.cart-toatl-price{
    float: left;
    width: 120rpx;
}

.cart-no-data{
    padding:40rpx 0;
    color: #999;
    text-align: center;
}
.ready{
  width: 150rpx;
  float: left;
  display: inline-block;
  margin-right: 20rpx;
  text-align: center
}
.readylists{
    width: 150rpx;
  float: right;
  display: inline-block;
  text-align: center
}
.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, .6);
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}
.contbox{
    width: 600rpx;
    height:300rpx;
    background: #fff;
    font-size: 28rpx;
    border-radius: 12rpx;
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    margin: auto;
    z-index: 999
}
.prompt{
  width: 100%;
  height:90rpx;
  border: none;
  outline: none;
  background: #fff; 
  z-index: 999
}
.obtn{
  width: 500rpx;
  height:80rpx;
  background: #00A465;
  border-radius: 12rpx;
  color: #fff;
}
.cancel{
  margin-top: 20rpx;
  width: 500rpx;
  height:80rpx;
  background: #00A465;
  color: #fff;
  border-radius: 12rpx;
  z-index: 999
}
.server-logo{
  height: 100rpx;
  width: 100rpx;
}