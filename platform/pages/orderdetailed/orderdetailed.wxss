page {
  display: flex;
  flex-direction: column;
  height: 100%;
}

.navbar {
  flex: none;
  display: flex;
  background: #fff;
  font-size: 30rpx;
}
.navbar .item {
  position: relative;
  flex: auto;
  text-align: center;
  line-height: 80rpx;
}
.navbar .item.active {
  color:#07bb04;
}
.navbar .item.active:after {
  content: "";
  display: block;
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 4rpx;
  background:#07bb04;
}
.allbox,
.sering,
.waitcommont,
.waitpay{
  position: absolute;
  top:80rpx;
  font-size: 28rpx;
  color: #555;
}
.appbox{
  margin: 20rpx auto;
  background: #efefed;
}
